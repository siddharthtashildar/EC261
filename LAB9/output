    %parti/s 1, 0, 2;
    %inv;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0000018f06b072d0_0, 4, 5;
    %load/vec4 v0000018f06b072d0_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0000018f06b072d0_0;
    %parti/s 1, 0, 2;
    %xor;
    %ix/load 4, 1, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0000018f06b072d0_0, 4, 5;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0000018f06b0bb90;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0000018f06b0beb0_0;
    %inv;
    %store/vec4 v0000018f06b0beb0_0, 0, 1;
    %jmp T_1;
